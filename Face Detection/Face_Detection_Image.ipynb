   "metadata": {},
   "outputs": [],
   "source": [
    "#Coverting image to gray scale\n",
    "imgGray = cv2.cvtColor(image, cv2.COLOR_BGR2GRAY)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 16,
   "id": "92d36b17",
   "metadata": {},
   "outputs": [],
   "source": [
    "#Face detetction \n",
    "faces = face_Cascade.detectMultiScale(imgGray,1.1, 4)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 17,
   "id": "13c77a2f",
   "metadata": {},
   "outputs": [],
   "source": [
    "#Show Rectangle on detected face\n",
    "for (x, y, w, h) in faces:\n",
    "    cv2.rectangle(image, (x, y), (x + w, y + h), (255, 0, 0), 2)"
   ]
  },
